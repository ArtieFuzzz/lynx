{
  "name": "@astriel-division/ts-config",
  "version": "1.0.7",
  "main": "tsconfig.json",
  "scripts": {
    "commit": "git-cz"
  },
  "dependencies": {
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@types/node": "16.7.8",
    "cz-conventional-changelog": "3.3.0",
    "rimraf": "3.0.2",
    "standard-version": "9.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AstrielDivision/ts.git"
  },
  "bugs": {
    "url": "https://github.com/AstrielDivision/ts/issues"
  },
  "files": [
    "tsconfig.json"
  ],
  "keywords": [
    "tsconfig",
    "config"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
